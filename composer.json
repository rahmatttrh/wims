{
  "name": "tecdiary/wims",
  "type": "project",
  "version": "1.4.9",
  "description": "Warehouse Inventory Management Solution",
  "keywords": [],
  "license": "MIT",
  "require": {
    "php": "^8.2",
    "guzzlehttp/guzzle": "^7.3",
    "inertiajs/inertia-laravel": "^2.0",
    "laravel/framework": "^12.0",
    "laravel/jetstream": "^5.0",
    "laravel/sanctum": "^4.0",
    "laravel/tinker": "^2.7",
    "rap2hpoutre/fast-excel": "^5.2",
    "reinink/remember-query-strings": "^0.1.0",
    "robinvdvleuten/ulid": "^5.0",
    "spatie/laravel-activitylog": "^4.10",
    "spatie/laravel-permission": "^6.4",
    "spatie/laravel-schemaless-attributes": "^2.5",
    "spatie/laravel-sluggable": "^3.7",
    "tecdiary/installer": "^0.1.0",
    "tecdiary/laravel-attachments": "^1.1",
    "tightenco/ziggy": "^2.4"
  },
  "require-dev": {
    "fakerphp/faker": "^1.9.1",
    "laravel/pint": "^1.13",
    "laravel/sail": "^1.0.1",
    "mockery/mockery": "^1.4.2",
    "nunomaduro/collision": "^8.6",
    "pestphp/pest": "^3.7",
    "pestphp/pest-plugin-laravel": "^3.1",
    "spatie/laravel-ignition": "^2.0",
    "stichoza/google-translate-php": "^5.1"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    },
    "files": [
      "app/Helpers/functions.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ]
  },
  "repositories": [
    {
      "type": "path",
      "url": "./packages/installer",
      "options": {
        "symlink": true
      }
    }
  ]
}
